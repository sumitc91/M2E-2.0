            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="skin-blue" ng-controller="beforeLoginMasterPageController" id="main" ng-cloak>
        <script data-main="../../App/Pages/PreLoginDM.js" src="../../App/js/require.min.js"></script>
        <!-- header logo: style can be found in header.less -->
        <header class="header">            
            <!-- Header Navbar: style can be found in header.less -->
            <nav role="navigation" class="navbar navbar-static-top-header" style="margin-left: 0;">
                <!-- Sidebar toggle button-->                                
                <div class="container">
                    <div>                       
                        <a class="navbar-brand-logo googleAnalytics" href="#" ga-category="BeforeLogin" ga-event="CompanyLogo" ga-label="Click"><img src="../../Template/AdminLTE-master/img/m2eV3.png" alt="Logo" width="125" height="39" /></a>
                    </div>
                    <div>                
                        <div class=" navbar-right googleAnalytics" style="line-height: 37px;" ga-category="BeforeLogin" ga-event="Login" ga-label="web" method="post" action="/Account/Login/web">
                            <a class="googleAnalytics" href="#/AboutUs" ga-category="BeforeLogin" ga-event="What is M2E" ga-label="LinkClick"><span style="color: #d6d6d6; font-size: 16px; font-weight: normal;" data-i18n="beforeLoginMasterPageAboutM2E"></span></a>
                            <a class="googleAnalytics" href="#/login" ga-category="BeforeLogin" ga-event="Sign In Link Clicked" ga-label="LinkClick"><span style="color: #d6d6d6; font-size: 16px; font-weight: normal; margin-left: 10px;" data-i18n="beforeLoginMasterPageSignIn"></span></a>
                        </div>
                    </div>
                </div>                                                                                
            </nav>
        </header>
        <div class="wrapper row-offcanvas row-offcanvas-left" style="min-height: 317px;" ng-view>           
        </div>       
        
        <div id="footer">
            <div class="container">
                <div class="text-muted credit">
                    <div style="float: left; margin-left: -16px auto;font-size: 16px; margin-top: -12px;">
                        <span ng-cloak>©2014 <span ng-bind="beforeLoginFooterCopyRightInfo.companyName"></span>
                            <span style="margin-left: 10px; padding-left: 1px;"><a href="#/contactus" class="googleAnalytics" ga-category="BeforeLogin" ga-event="ContactUsBottonLink" ga-label="Click" ><span ng-bind="beforeLoginFooterCopyRightInfo.contactUs"></span></a></span>
                            <span style="margin-left: 2px; padding-left: 1px;">|</span>
                            <span style="margin-left: 2px; padding-left: 1px;"><a href="#/faq" class="googleAnalytics" ga-category="BeforeLogin" ga-event="FAQBottonLink" ga-label="Click"><span ng-bind="beforeLoginFooterCopyRightInfo.FAQ"></span></a></span>
                            <span style="margin-left: 2px; padding-left: 1px;">|</span>
                            <span style="margin-left: 2px; padding-left: 1px;"><a href="" class="googleAnalytics" ga-category="BeforeLogin" ga-event="Terms&PrivacyBottonLink" ga-label="Click"><span ng-bind="beforeLoginFooterCopyRightInfo.TnC"></span></a></span>
                        </span> 
                    </div>
                </div>
            </div>
        </div>
        <script>
            $(function () {
                window.madetoearn.i18nLocalizePage();
            });
        </script>
           
    </body>
