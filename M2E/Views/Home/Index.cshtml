<style>

    .footerMinHeight {
        min-height: 500px !important;
    }
    #page-footer #footer-bottom {
    background-color: #012447;
    color: #fff;
    padding: 20px 0;
}
 #page-footer_mobile {
    background-color: #012447;
    color: #fff;
    padding: 30px 0;
}
    #page-footer #footer-bottom .footer-inner {
    display: table;
    width: 100%;
}
    #page-footer #footer-bottom .footer-inner .copyright {
    display: table-cell;
    float: none;
    vertical-align: middle;
    width: 50%;
    text-align: center;
}
    #page-footer #footer-bottom .footer-inner .design {
    display: table-cell;
    float: right;
    text-align: right;
    vertical-align: middle;
    width: 50%;
}
 .middleFooterBackground {
    background-color: #004c8d;
    padding: 20px 0;
    position: relative;
}

.background {
    height: 100%;
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: -1;
}

#page-footer h4:after {
    bottom: -15px;
    content: "";
    height: 2px;
    left: 0;
    opacity: 0.7;
    position: absolute;
    width: 30px;
    z-index: 0;
}
*:before, *:after {
    box-sizing: border-box;
}
#page-footer h4 {
    color: #fff;
    display: inline-block;
    font-size: 20px;
    margin-bottom: 35px;
    margin-top: 10px;
    position: relative;
}
#page-footer #footer-content {
    color: #fff !important;
    font-size: small;
}
#page-footer ul {
    color: #fff;
}
ul.list-links {
    color: #d9d9d9;
    padding-left: 15px;
}
ul.list-links li {
    padding: 0px;
 }

#page-footer #footer-content a {
    color: #fff;
}
#page-footer a {
    color: #fff !important;
    font-size: 13px;
}
ul.list-links a {
    display: inline-block;
    left: 0;
    padding: 6px 0;
    position: relative;
}
a {
    color: #01366a;
    outline: medium none !important;
    transition: all 0.3s ease 0s;
}
#page-footer 
a:hover {
    color: #0979cd !important;
    left: 3px;
}
</style>